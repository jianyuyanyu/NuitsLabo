<Window x:Class="WpfWithSimpleInjector.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWithSimpleInjector"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" 
        Activated="Window_Activated"
        Initialized="MainWindow_OnInitialized"
        Loaded="MainWindow_OnLoaded"
        Closed="MainWindow_OnClosed"
        Closing="MainWindow_OnClosing"
        Deactivated="MainWindow_OnDeactivated"
        FocusableChanged="MainWindow_OnFocusableChanged"
        GotFocus="MainWindow_OnGotFocus"
        IsVisibleChanged="MainWindow_OnIsVisibleChanged"
        LayoutUpdated="MainWindow_OnLayoutUpdated"
        LostFocus="MainWindow_OnLostFocus"
        SizeChanged="MainWindow_OnSizeChanged"
        SourceInitialized="MainWindow_OnSourceInitialized"
        SourceUpdated="MainWindow_OnSourceUpdated"
        Unloaded="MainWindow_OnUnloaded"
        >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="39*"/>
            <ColumnDefinition Width="121*"/>
        </Grid.ColumnDefinitions>

    </Grid>
</Window>
